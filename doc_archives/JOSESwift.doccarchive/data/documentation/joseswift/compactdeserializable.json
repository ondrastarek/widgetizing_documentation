eserializable","abstract":[],"role":"symbol","type":"topic","navigatorTitle":[{"text":"CompactDeserializable","kind":"identifier"}],"identifier":"doc:\/\/JOSESwift\/documentation\/JOSESwift\/CompactDeserializable","url":"\/documentation\/joseswift\/compactdeserializable","fragments":[{"kind":"keyword","text":"protocol"},{"text":" ","kind":"text"},{"text":"CompactDeserializable","kind":"identifier"}]},"doc://JOSESwift/documentation/JOSESwift/JWE":{"title":"JWE","identifier":"doc:\/\/JOSESwift\/documentation\/JOSESwift\/JWE","navigatorTitle":[{"text":"JWE","kind":"identifier"}],"fragments":[{"text":"struct","kind":"keyword"},{"text":" ","kind":"text"},{"kind":"identifier","text":"JWE"}],"type":"topic","url":"\/documentation\/joseswift\/jwe","kind":"symbol","role":"symbol","abstract":[{"text":"A JWE consisting of five parameters as specified in ","type":"text"},{"identifier":"https:\/\/tools.ietf.org\/html\/rfc7516","type":"reference","isActive":true},{"type":"text","text":"."},{"type":"text","text":" "},{"text":"The JWE is fully initialized with those five (immutable) parameters.","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"All representations of the JWE or it’s parts like it’s compact serialization or the plaintext can be derived from those five parameters."},{"text":" ","type":"text"},{"text":"Therefore (and to keep immutability) it does not cache such representations.","type":"text"},{"type":"text","text":" "},{"type":"text","text":"As discussed, it is the responsibility of the framework user to cache e.g. the plaintext. Of course this will have to be communicated clearly."}]}}}