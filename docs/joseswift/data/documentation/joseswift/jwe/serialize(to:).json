"doc://JOSESwift/documentation/JOSESwift/JWE":{"title":"JWE","kind":"symbol","abstract":[{"type":"text","text":"A JWE consisting of five parameters as specified in "},{"isActive":true,"type":"reference","identifier":"https:\/\/tools.ietf.org\/html\/rfc7516"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"The JWE is fully initialized with those five (immutable) parameters."},{"text":" ","type":"text"},{"text":"All representations of the JWE or it’s parts like it’s compact serialization or the plaintext can be derived from those five parameters.","type":"text"},{"text":" ","type":"text"},{"type":"text","text":"Therefore (and to keep immutability) it does not cache such representations."},{"text":" ","type":"text"},{"text":"As discussed, it is the responsibility of the framework user to cache e.g. the plaintext. Of course this will have to be communicated clearly.","type":"text"}],"url":"\/documentation\/joseswift\/jwe","identifier":"doc:\/\/JOSESwift\/documentation\/JOSESwift\/JWE","navigatorTitle":[{"kind":"identifier","text":"JWE"}],"type":"topic","role":"symbol","fragments":[{"text":"struct","kind":"keyword"},{"kind":"text","text":" "},{"kind":"identifier","text":"JWE"}]},"doc://JOSESwift/documentation/JOSESwift/CompactSerializer":{"url":"\/documentation\/joseswift\/compactserializer","type":"topic","navigatorTitle":[{"kind":"identifier","text":"CompactSerializer"}],"kind":"symbol","role":"symbol","identifier":"doc:\/\/JOSESwift\/documentation\/JOSESwift\/CompactSerializer","abstract":[],"fragments":[{"kind":"keyword","text":"protocol"},{"text":" ","kind":"text"},{"text":"CompactSerializer","kind":"identifier"}],"title":"CompactSerializer"},"doc://JOSESwift/documentation/JOSESwift":{"url":"\/documentation\/joseswift","type":"topic","kind":"symbol","role":"collection","abstract":[],"identifier":"doc:\/\/JOSESwift\/documentation\/JOSESwift","title":"JOSESwift"},"doc://JOSESwift/documentation/JOSESwift/JWE/CompactSerializable-Implementati